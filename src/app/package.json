{
  "name": "trpg-game-app",
  "version": "0.2.31",
  "appVersion": "0.2.9",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run dev:main",
    "dev:local": "node build/utils/get-local-ip.js | xargs -I {} cross-env TRPG_HOST={}:23256 npm run dev:main",
    "dev:proserver": "cross-env TRPG_HOST=trpgapi.moonrailgun.com:443 npm run dev:main",
    "dev:main": "npm run build:env && cross-env PLATFORM=app REACT_NATIVE_APP_ROOT=../../ react-native start --reset-cache",
    "dev:android": "react-native run-android",
    "build:env": "cross-env NODE_CONFIG_DIR=../../config/ node ./build/metro/build-env.js && node ./build/metro/build-version.js",
    "build:android": "npm run build:env && cd ./android && cross-env NODE_ENV=production PLATFORM=app SENTRY_PROJECT=trpg-client-web REACT_NATIVE_APP_ROOT=../../ ./gradlew assembleRelease",
    "deploy": "npm run build:env && node build/deploy/deploy.js",
    "dump:android-home": "echo \"sdk.dir=$ANDROID_HOME\" >> ./android/local.properties",
    "assets:update": "bash ./build/copy-iconfont.sh",
    "test": "node ../../node_modules/jest/bin/jest.js"
  },
  "keywords": [
    "trpg"
  ],
  "author": "moonrailgun",
  "license": "GPL-3.0",
  "dependencies": {
    "@ant-design/icons-react-native": "^1.0.2",
    "@ant-design/react-native": "^3.1.15",
    "@react-native-community/cameraroll": "^1.2.1",
    "apsl-react-native-button": "^3.1.1",
    "react-native": "^0.60.4",
    "react-native-camera": "^3.8.0",
    "react-native-code-push": "5.6.0",
    "react-native-config": "^0.11.7",
    "react-native-emoji": "git+https://github.com/TRPGEngine/react-native-emoji.git",
    "react-native-fast-image": "^7.0.2",
    "react-native-fs": "^2.14.1",
    "react-native-gesture-handler": "^1.2.0",
    "react-native-image-crop-picker": "^0.24.1",
    "react-native-image-picker": "^2.3.1",
    "react-native-image-progress": "^1.1.1",
    "react-native-image-zoom-viewer": "^2.2.25",
    "react-native-pdf": "^5.1.7",
    "react-native-permissions": "^1.1.1",
    "react-native-progress": "^3.6.0",
    "react-native-qrcode-scanner": "^1.2.3",
    "react-native-render-html": "^4.2.0",
    "react-native-root-siblings": "^4.0.6",
    "react-native-root-toast": "^3.0.1",
    "react-native-sentry": "^0.42.0",
    "react-native-style-block": "0.0.4",
    "react-native-webrtc": "^1.75.3",
    "react-native-webview": "^5.7.2",
    "react-navigation": "^3.9.1",
    "react-navigation-redux-helpers": "^3.0.2",
    "react-navigation-stack": "^1.4.0",
    "rn-fetch-blob": "^0.11.2",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@types/inquirer": "^6.5.0",
    "code-push-cli": "^2.1.9",
    "cross-env": "^5.2.1",
    "execa": "^2.0.4",
    "inquirer": "^7.0.0",
    "metro-react-native-babel-preset": "^0.56.0"
  }
}
